<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chainclothingproject.Customer.ReturnOrCancelltemController">
   <children>
      <Label layoutX="195.0" layoutY="22.0" text="Return Or Cancelltem">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button layoutX="10.0" layoutY="100.0" mnemonicParsing="false" onAction="#myOrderOA" text="My Order" />
      <TableView fx:id="orderTableView" layoutX="9.0" layoutY="143.0" prefHeight="157.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="itemTC" prefWidth="75.0" text="item" />
          <TableColumn fx:id="statusTC" prefWidth="75.0" text="status" />
        </columns>
      </TableView>
      <TextField fx:id="idTextfield" layoutX="89.0" layoutY="58.0" />
      <RadioButton fx:id="returnRB" layoutX="401.0" layoutY="71.0" mnemonicParsing="false" text="Return" />
      <RadioButton fx:id="cancelRB" layoutX="401.0" layoutY="104.0" mnemonicParsing="false" text="Cancel" />
      <Label layoutX="306.0" layoutY="71.0" text="Return/Cancle?" />
      <ComboBox fx:id="reasonCombobox" layoutX="355.0" layoutY="153.0" prefWidth="150.0" promptText="reason" />
      <Label layoutX="34.0" layoutY="58.0" text="Order id" />
      <Button layoutX="55.0" layoutY="311.0" mnemonicParsing="false" onAction="#submitOA" text="Submit" />
   </children>
</AnchorPane>
