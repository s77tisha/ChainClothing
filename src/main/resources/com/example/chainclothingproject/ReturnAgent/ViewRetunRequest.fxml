<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="ReturnAgent.ViewReturnRequestController">
    <Label layoutX="20" layoutY="20" style="-fx-font-size: 18px;" text="View Return Requests" />
    <TableView fx:id="returnRequestTable" layoutX="20" layoutY="60" prefHeight="400" prefWidth="600">
        <columns>
            <TableColumn fx:id="orderIDColumn" text="Order ID" />
            <TableColumn fx:id="customerNameColumn" prefWidth="103.20001983642578" text="Customer Name" />
            <TableColumn fx:id="productColumn" prefWidth="108.79998779296875" text="Product" />
            <TableColumn fx:id="reasonColumn" prefWidth="97.5999755859375" text="Return Reason" />
            <TableColumn fx:id="statusColumn" prefWidth="153.59988403320312" text="Status" />
        </columns>
    </TableView>
    <Button layoutX="20" layoutY="480" onAction="#goBack" text="Back" />
</AnchorPane>
